{% extends 'baseBack.html.twig' %}

{% block title %}Zone{% endblock %}

{% block body %}
    <h1>Zone</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ zone.id }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ zone.location }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
